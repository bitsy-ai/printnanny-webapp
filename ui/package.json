{
  "name": "printnanny-ui",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "npm run api-client-build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "vite --mode dev",
    "ci-webapp": "cd ../ && make ci-up",
    "ci-webapp-wait": "wait-on http://localhost:8000",
    "api-client-build": "cd ../clients/typescript && npm install && npm run build",
    "ci-collectstatic": "cd .. && make ci-collectstatic",
    "dev-build": "vite build --base=/ui/ --mode dev",
    "build": "run-p lint type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e:open": "cypress open --e2e",
    "test:e2e:run": "cypress run --e2e",
    "test:e2e:ci": "cypress run --e2e --config baseUrl=http://localhost:8000 --record ${CYPRESS_RECORD_KEY}",
    "build-only": "vite build --base=/ui/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore .",
    "lint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore ."
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "^2.0.14",
    "@stripe/stripe-js": "^1.36.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "browserify-fs": "^1.0.0",
    "events": "^3.3.0",
    "janode": "github:bitsy-ai/janode#2022-08-21-rm-unix-connection",
    "moment": "^2.29.4",
    "nats.ws": "^1.8.1",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.6.3",
    "posthog-js": "^1.34.0",
    "printnanny-api-client": "file:../clients/typescript",
    "uuid4": "^2.0.3",
    "vee-validate": "^4.6.1",
    "vue": "^3.2.37",
    "vue-3d-model": "^2.0.0-alpha.2",
    "vue-router": "^4.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.43",
    "@types/uuid4": "^2.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^12.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-radar": "^0.5.0",
    "vitest": "^0.18.0",
    "vue-tsc": "^0.38.4",
    "wait-on": "^7.0.1"
  }
}