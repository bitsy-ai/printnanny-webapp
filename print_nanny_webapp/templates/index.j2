{% load django_vite %}
{% load feature_flags %}
{% flag_enabled 'MONTHLY_SUB' as FF__MONTHLY_SUB %}

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="https://rsms.me/inter/inter.css">

  <link rel="icon" href="https://cdn.printnanny.ai/www/favicon.svg" type="image/svg+xml">
  <link rel="icon" type="image/png" href="https://cdn.printnanny.ai/www/favicon.png">
  <title>PrintNanny</title>
  <script>
    {% if FF__MONTHLY_SUB %}
    window.FF__MONTHLY_SUB = true;
    {% else %}
    window.FF__MONTHLY_SUB = false;
    {% endif %}
  </script>
  {% vite_hmr_client %}
  {% vite_asset 'src/main.ts' %}
</head>

<body>
  <div id="app"></div>
  <script type="module" src="/src/main.ts"></script>
</body>

</html>